<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.learning">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_android_enabled"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".course.ui.CoursesActivity" />
        <activity
            android:name=".instructor.ui.InstructorActivity"
            android:label="@string/title_activity_instructor"
            android:theme="@style/AppTheme" />
        <activity android:name=".course.ui.CourseActivity" />
        <activity android:name=".instructor.ui.InstructorsActivity"></activity>
    </application>

</manifest>